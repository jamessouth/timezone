    "build": "webpack --env.ENV=prod && cp -r src/assets/icons/ dist/",
    "test": "nyc riteway -r @babel/register -r @babel/polyfill src/**/*.test.js | tap-nirvana",
    "watch": "watch \"clear && npm run -s lint && npm run -s test\" src/"
  },
